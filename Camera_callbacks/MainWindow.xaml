<Window x:Class="Camera_callbacks.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Camera_callbacks"
        xmlns:vlc="clr-namespace:LibVLCSharp.WPF;assembly=LibVLCSharp.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    <StackPanel>
        <vlc:VideoView x:Name="VideoView" Height="270" Width="480"/>
        <TextBox x:Name="lockTextBox" Height="20" Margin="10"></TextBox>
        <TextBox x:Name="displayTextBox" Height="20" Margin="10"></TextBox>
    </StackPanel>
</Window>
